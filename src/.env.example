# API Settings
endpoint_300=https://api.example.com/summarize  # Summarization API endpoint
token_300=your_summarization_api_token          # Summarization API token
RPS=2                                          # Maximum requests per second
CONSTRUCTOR_KM_API=https://api.example.com/news  # News digest API endpoint
CONSTRUCTOR_KM_API_KEY=your_news_api_key       # News digest API key
MODEL=gpt-4                                    # AI model name for classification

# S3 Settings
BUCKET_NAME=your-bucket-name                   # S3 bucket name
ENDPOINT_URL=https://storage.example.com       # S3-compatible storage URL
ACCESS_KEY=your_access_key                     # S3 access key
SECRET_KEY=your_secret_key                     # S3 secret key
rss_300_file_name=feed.rss                     # RSS feed file name for storage

# RSS Settings
logo_url=https://example.com/logo.png          # Default logo URL
RSS_LINKS=https://example.com/rss-list.txt     # URL of RSS feeds list
feed_url=https://example.com/feed.xml          # Main RSS feed URL

# Telegram Settings
TELEGRAM_BOT_TOKEN=your_telegram_bot_token     # Telegram bot token
TELEGRAM_CHAT_ID=your_chat_id                  # Chat ID for notifications
TELEGRAM_CHAT_ID_NEWS=your_news_chat_id        # Chat ID for news digest
TELEGRAPH_ACCESS_TOKEN=your_telegraph_token     # Telegraph API access token
